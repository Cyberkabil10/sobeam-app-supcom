        "type": "query",
        "name": "select oce1_0.id,oce1_0.activate_user,oce1_0.additional_info,oce1_0.allow_user_creation,oce1_0.basic_always_full_screen,oce1_0.authorization_uri,oce1_0.client_authentication_method,oce1_0.client_id,oce1_0.client_secret,oce1_0.created_time,oce1_0.basic_customer_name_pattern,oce1_0.basic_default_dashboard_name,oce1_0.basic_email_attribute_key,oce1_0.basic_first_name_attribute_key,oce1_0.jwk_set_uri,oce1_0.basic_last_name_attribute_key,oce1_0.login_button_icon,oce1_0.login_button_label,oce1_0.custom_password,oce1_0.platforms,oce1_0.scope,oce1_0.custom_send_token,oce1_0.tenant_id,oce1_0.basic_tenant_name_pattern,oce1_0.basic_tenant_name_strategy,oce1_0.title,oce1_0.token_uri,oce1_0.type,oce1_0.custom_url,oce1_0.user_info_uri,oce1_0.user_name_attribute_name,oce1_0.custom_username from oauth2_client oce1_0 left join domain_oauth2_client doce1_0 on oce1_0.id=doce1_0.oauth2_client_id left join domain de1_0 on doce1_0.domain_id=de1_0.id where de1_0.name=? and de1_0.oauth2_enabled=true and (? is null or oce1_0.platforms is null or oce1_0.platforms='' or (oce1_0.platforms::text ILIKE ('%'||?||'%')::text)=true)"
      }
